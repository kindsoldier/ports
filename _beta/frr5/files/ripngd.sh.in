#!/bin/sh
#
# PROVIDE: ripngd
# REQUIRE: zebra
#
. /etc/rc.subr

name="ripngd"
rcvar="ripngd_enable"
command="%%PREFIX%%/sbin/${name}"
pidfile="%%RUN_DIR%%/${name}.pid"
procname="${name}"
required_dirs="%%RUN_DIR%% %%LOG_DIR%%"
required_files="%%CONF_DIR%%/${name}.conf"
command_args=" -d"

load_rc_config $name

: ${ripngd_enable="NO"}

run_rc_command "$1"
#EOF
