+{ $as_echo "$as_me:$LINENO: checking for sig_atomic_t type" >&5
+$as_echo_n "checking for sig_atomic_t type... " >&6; }
+if test "${libreplace_cv_sig_atomic_t+set}" = set; then
+  $as_echo_n "(cached) " >&6
+else
