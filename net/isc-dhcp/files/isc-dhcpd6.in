#!/bin/sh
#
# PROVIDE: isc-dhcpd6
# BEFORE: LOGIN
#
. /etc/rc.subr

name="isc_dhcpd6"
rcvar="isc_dhcpd6_enable"
command="%%PREFIX%%/sbin/isc-dhcpd"
procname="${command}"
required_files="%%PREFIX%%/etc/dhcpd6.conf"
command_args="-q -6 -cf %%PREFIX%%/etc/dhcpd6.conf"

load_rc_config $name

: ${isc_dhcpd6_enable="NO"}

run_rc_command "$1"
#EOF
