#!/bin/sh
#
# PROVIDE: nfcapd0
# REQUIRE: DAEMON
#
. /etc/rc.subr

#instance=0

name="nfcapd0"
rcvar="nfcapd0_enable"
command="/usr/local/sbin/nfcapd"
pidfile="/var/run/nfcapd0.pid"
procname="${command}"
#command_args=" -D -P ${pidfile}"
required_dirs="/var/run"
#required_vars="${name}_interfaces ${name}_netflow_host ${name}_netflow_port"

load_rc_config $name

# /var/db/nfdump2 -S 8 -p 9998 -P /var/run/nfcapd2.pid

: ${nfcapd0_enable="NO"}
: ${nfcapd0_flags="-D -p 9995 -l /var/db/nfdump0 -P /var/run/nfcapd0.pid -S 8  "}


run_rc_command "$1"
#EOF
