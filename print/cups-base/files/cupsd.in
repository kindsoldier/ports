#!/bin/sh
#
# $FreeBSD: head/print/cups-base/files/cupsd.in 372260 2014-11-07 13:03:09Z tijl $
#
# PROVIDE: cupsd
# REQUIRE: DAEMON avahi_daemon
# KEYWORD: shutdown
#
# Add the following to /etc/rc.conf[.local] to enable this service
#
# cupsd_enable="YES"
#

. /etc/rc.subr

name="cupsd"
rcvar="cupsd_enable"

start_precmd="cupsd_prestart"

command="%%PREFIX%%/sbin/cupsd"
extra_commands="reload"

cupsd_prestart()
{
	if [ -n "$TZ" ]; then
		export TZ
	fi
}

load_rc_config ${name}

: ${cupsd_enable="NO"}

run_rc_command "$1"
#EOF
